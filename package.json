{
    "name": "angular2-jwt-authentication-example",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/cornflourblue/angular2-jwt-authentication-example.git"
    },
    "scripts": {
        "start": "tsc && concurrently \"npm run build:sass\" \"npm run tsc:w\" \"npm run lite\" \"npm run watch:sass\"",
        "lite": "lite-server",
        "tsc": "tsc",
        "tsc:w": "tsc -w",
        "build:sass": "node-sass assets/sass/ -o assets/css",
        "watch:sass": "npm run build:sass && node-sass assets/sass// -wo assets/css/"
    },
    "license": "MIT",
    "dependencies": {
        "@angular/common": "^4.0.0",
        "@angular/compiler": "^4.0.0",
        "@angular/core": "^4.0.0",
        "@angular/forms": "^4.0.0",
        "@angular/http": "^4.0.0",
        "@angular/platform-browser": "^4.0.0",
        "@angular/platform-browser-dynamic": "^4.0.0",
        "@angular/router": "^4.0.0",
        "core-js": "^2.4.1",
        "rxjs": "^5.2.0",
        "systemjs": "^0.19.47",
        "zone.js": "^0.8.5"
    },
    "devDependencies": {
        "@types/node": "^6.0.60",
        "concurrently": "^3.1.0",
        "lite-server": "^2.3.0",
        "node-sass": "^4.5.3",
        "typescript": "^2.2.2"
    }
}
